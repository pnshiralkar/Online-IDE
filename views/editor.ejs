<body>
<form method="post" action="/save" id="frm_save" target="_blank"><input type="hidden" id="inp_save" name="code"/>
<button id="save" class="btn btn-primary border-white" style="position: fixed; bottom: 21.4vh;right: 1.4vh; z-index: 1; font-size: larger"><b>Save</b></button>
</form>
<button id="btn_inp" class="btn btn-success border-white" style="position: fixed; bottom: 21.4vh;right: 11vh; z-index: 1; font-size: larger"><b>Run</b></button>
<span id="mobile_test_element"></span>
<div class="container" style="width:100%;padding:0px;margin:0px">

    <div class="col-sm-12" style="margin:0px;padding:0px">
        <div style="height:100vh">
            <div class="split-pane fixed-left">

                <div class="split-pane-divider hide" id="my-divider"></div>
                <div class="split-pane-component" id="right-component">


                    <input type="hidden" id="snippet_id" value="">

                    <div class="split-pane fixed-left">
                        <div class="split-pane-component" id="right-left-component">
                            <div id="top-component">

                                <div id="editor-container">


                                    <div id="multi_editor_container">


                                        <div id="editor_1" class="tab-pane active editor"
                                             style="position: absolute;width:100%;top:0">
/******************************************************************************
    C++ Compiler in Node.js
    A project by Prathamesh Shiralkar.
*******************************************************************************/

