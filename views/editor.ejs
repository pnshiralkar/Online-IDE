{
    cout&lt;&lt;&quot;Hello World&quot;;

    return 0;
}
</div>

                                    </div>
                                </div>
                                <!-- middle top end -->
                            </div>
                            <div class="split-pane-component" id="bottom-component" style="height: 20vh; padding: 8px; background-color: black;color: white;overflow-y: scroll">
                                Output :
                                <pre style="background-color: #000000;display:none;" id="pre_err">
                                <div id="errors" style=" font-family: monospace;width: 100%;margin: 5px;color: red">

                                </div></pre>
                                <pre style="background-color: #000000;display:none" id="pre_op">
                                <div id="outputs" style=" font-family: monospace;width: 100%;margin: 5px;color: #ffffff">

                                </div></pre>
                            </div>

                            <script>



                            </script>




                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="popup" id="popup">
    <div class="popup_contents">
        <div class="popup_title">
        <button type="button" class="close" aria-label="Close" id="popup_close">
            <span aria-hidden="true">&times;</span>
        </button>
            <h1>Input:</h1>
        </div>
        <div class="popup_cont">
            <textarea class="inp" id="inp"></textarea>
            <button id="btn_run" class="btn btn-success border-white" style="position: relative;margin: 4vh;float: right; z-index: 1; font-size: larger"><b>Run</b></button>
        </div>
    </div>
</div>


